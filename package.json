{
    "name": "@humen-team/express-openapi",
    "version": "0.8.4",
    "description": "Opinionated REST APIs for express with OpenAPI support.",
    "repository": "https://github.com/humen-team/express-openapi",
    "author": "Humen Team",
    "license": "MIT",
    "main": "lib/index.js",
    "scripts": {
        "prepublish": "rm -rf lib && babel src --out-dir lib --ignore '**/__tests__/*,**/__mocks__/*'",
        "lint": "eslint src --cache",
        "test": "jest",
        "test:start": "babel-node ./src/__tests__/app.js"
    },
    "dependencies": {
        "http-status-codes": "^1.3.2",
        "jsonschema": "^1.2.4",
        "lodash": "^4.17.15",
        "multer": "^1.4.2"
    },
    "devDependencies": {
        "@babel/cli": "^7.5.5",
        "@babel/core": "^7.5.5",
        "@babel/node": "^7.5.5",
        "@babel/plugin-proposal-optional-chaining": "^7.2.0",
        "@babel/plugin-transform-runtime": "^7.5.5",
        "@babel/preset-env": "^7.5.5",
        "@babel/runtime": "^7.5.5",
        "babel-eslint": "^10.0.2",
        "cors": "^2.8.5",
        "eslint": "^6.1.0",
        "eslint-config-airbnb-base": "^14.0.0",
        "eslint-plugin-import": "^2.18.2",
        "express": "^4.17.1",
        "jest": "^24.8.0",
        "supertest": "^4.0.2"
    },
    "peerDependencies": {
        "express": "^4.17.1"
    },
    "jest": {
        "modulePaths": [
            "<rootDir>/src"
        ],
        "testRegex": "/__tests__/.*test\\.js$"
    }
}
